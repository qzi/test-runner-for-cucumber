{
	"name": "test-runner-for-cucumber",
	"displayName": "Test Runner for Cucumber",
	"description": "Test Runner for Cucumber/Gherkin",
	"version": "0.2.1",
	"publisher": "Leon Qiu",
	"private": true,
	"license": "Apache License 2.0",
	"icon": "images/cucumber-ext-v0.2.png",
	"repository": {
		"type": "git",
		"url": ""
	},
	"engines": {
		"vscode": "^1.58.0"
	},
	"categories": [
		"Programming Languages",
		"Testing",
		"Education"
	],
	"keywords": [
		"cucumber",
		"bdd",
		"gherkin",
		"snippets"
	],
	"activationEvents": [
		"onCommand:cucumberRunner.runCurrentFeature"
	],
	"main": "./out/extension.js",
	"contributes": {
		"commands": [
			{
				"command": "cucumberRunner.runCurrentFeature",
				"title": "Cucumber Runner: runCurrentFeature"
			}
		]
	},
	"scripts": {
		"vscode:prepublish": "npm run compile",
		"compile": "tsc -p ./",
		"watch": "tsc -watch -p ./",
		"lint": "eslint . --ext .ts,.tsx"
	},
	"devDependencies": {
		"@types/node": "^16.11.7",
		"@types/vscode": "^1.58.0",
		"@typescript-eslint/eslint-plugin": "^5.30.0",
		"@typescript-eslint/parser": "^5.30.0",
		"esbuild": "^0.15.5",
		"eslint": "^8.13.0",
		"save": "^2.5.0",
		"spawn-command": "^0.0.2-1",
		"tree-kill": "^1.2.2",
		"typescript": "^4.7.2"
	}
}